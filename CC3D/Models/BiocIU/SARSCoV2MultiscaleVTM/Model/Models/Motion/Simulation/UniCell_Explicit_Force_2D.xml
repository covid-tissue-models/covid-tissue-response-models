<CompuCell3D Revision="0" Version="4.3.1">
   
   <Metadata>
      
      <!-- Basic properties simulation -->
      <NumberOfProcessors>1</NumberOfProcessors>
      <DebugOutputFrequency>10</DebugOutputFrequency>
      <!-- <NonParallelModule Name="Potts"/> -->
   </Metadata>
   
   <Potts>
      
      <!-- Basic properties of CPM (GGH) algorithm -->
      <Dimensions x="400" y="400" z="1"/>
      <Steps>50000</Steps>
      <Temperature>10.0</Temperature>
      <NeighborOrder>2</NeighborOrder>
      <Boundary_x>Periodic</Boundary_x>
      <Boundary_y>Periodic</Boundary_y>
   </Potts>
   
   <Plugin Name="CellType">
      
      <!-- Listing all cell types in the simulation -->
      <CellType TypeId="0" TypeName="Medium"/>
      <CellType TypeId="1" TypeName="UniCell"/>
   </Plugin>
   
   <Plugin Name="Volume">
   <VolumeEnergyParameters CellType="UniCell" LambdaVolume="1.0" TargetVolume="50"/>
</Plugin>
    
    <Plugin Name="NeighborTracker">
   
   <!-- Module tracking neighboring cells of each cell -->
</Plugin>

   
   <Plugin Name="ExternalPotential">
      
      <!-- External force applied to cell. Each cell has different force and force components have to be managed in Python. -->
      <!-- e.g. cell.lambdaVecX=0.5; cell.lambdaVecY=0.1 ; cell.lambdaVecZ=0.3; -->
      <Algorithm>CenterOfMassBased</Algorithm>
   </Plugin>
   
   <Plugin Name="CenterOfMass">
      
      <!-- Module tracking center of mass of each cell -->
   </Plugin>
   
   <Plugin Name="Contact">
      <!-- Specification of adhesion energies -->
      <Energy Type1="Medium" Type2="Medium">5.0</Energy>
      <Energy Type1="Medium" Type2="UniCell">5.0</Energy>
      <Energy Type1="UniCell" Type2="UniCell">5.0</Energy>
      <NeighborOrder>4</NeighborOrder>
   </Plugin>
   
   <Steppable Type="UniformInitializer">
      
      <Region>
         <BoxMin x="0" y="0" z="0"/>
         <BoxMax x="400" y="400" z="1"/>
         <Gap>15</Gap>
         <Width>5</Width>
         <Types>UniCell</Types>
      </Region>
   </Steppable>
</CompuCell3D>
